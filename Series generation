#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

       int n;
    scanf("%d",&n);
    int t1=7,t2=5;
    for(int i=0;i<n;i++)
    {
        if(i%2==0)
        {
            printf("%d ",t1);
            t1 +=1;
        }
        else
        {
            printf("%d ",t2);
            t2 +=1;
        }
    }
    return 0;
}
