#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

     int n;
    scanf("%d",&n);
    if(n<100||n>200)
    {
        printf("INVALID INPUT");
    }
    else
    {
        int num=n/4;
        int rem=n%4;
        int d=num+rem;
        if(rem==0)
        {
            printf("%d\n%d\n%d\n%d",num,num,num,num);
        }
        else
        {
            printf("%d\n%d\n%d\n%d",num,num,num,d);
        }
    }
    return 0;
}
